<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.dbc.ZHXYSystem.Entity.MainwheelnoticeEntity" table="mainwheelnotice" schema="zhxysystem">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="image">
            <column name="image" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
        <property name="descripton">
            <column name="descripton" sql-type="varchar(100)" length="100" not-null="true"/>
        </property>
        <property name="url">
            <column name="url" sql-type="varchar(200)" length="200" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>